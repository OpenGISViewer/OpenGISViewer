<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.opengisviewer.opengisviewer.controllers.DataPanelController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox BorderPane.alignment="CENTER" >
        <Label text="Data URL" />
        <TextArea fx:id="urlTextArea" prefHeight="10.0" prefWidth="200.0" />
        <Button fx:id="findUrlButton" mnemonicParsing="false" text="Find" onMouseClicked="#getUrl"/>
        <TreeView fx:id="dataTree" prefHeight="345.0" prefWidth="157.0" BorderPane.alignment="CENTER" />
    </VBox>
</AnchorPane>
